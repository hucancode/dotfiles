{
	"editor.codeLens": false,
	"editor.dragAndDrop": false,
	"editor.folding": false,
	"editor.fontFamily": "'Fira Code', monospace",
	"editor.fontLigatures": "'zero','cv14','cv26','cv30','cv31','cv32','ss03','ss04','ss06','ss10'",
	"editor.guides.indentation": false,
	"editor.hover.enabled": false,
	"editor.inlayHints.enabled": "off",
	"editor.lightbulb.enabled": "off",
	"editor.matchBrackets": "never",
	"editor.minimap.enabled": false,
	"editor.occurrencesHighlight": "off",
	"editor.overviewRulerBorder": false,
	"editor.scrollbar.horizontal": "hidden",
	"editor.scrollbar.vertical": "hidden",
	"editor.selectionHighlight": false,
	"editor.tabSize": 2,
	"editor.wordWrap": "on",
	"explorer.confirmDelete": false,
	"explorer.excludeGitIgnore": true,
	"extensions.ignoreRecommendations": true,
	"git.confirmSync": false,
	"git.openRepositoryInParentFolders": "never",
	"remote.autoForwardPorts": false,
	"rust-analyzer.lens.enable": false,
	"rust-analyzer.semanticHighlighting.nonStandardTokens": false,
	"rust-analyzer.semanticHighlighting.operator.enable": false,
	"rust-analyzer.semanticHighlighting.strings.enable": false,
	"screencastMode.fontSize": 42,
	"screencastMode.keyboardOptions": {
		"showCommands": false
	},
	"screencastMode.keyboardOverlayTimeout": 2500,
	"screencastMode.mouseIndicatorSize": 40,
	"screencastMode.verticalOffset": 10,
	"svelte.ask-to-enable-ts-plugin": false,
	"terminal.integrated.enableFileLinks": "off",
	"terminal.integrated.enablePersistentSessions": false,
	"terminal.integrated.fontFamily": "'Fira Code', 'FiraCode Nerd Font', monospace",
	"terminal.integrated.hideOnStartup": "always",
	"terminal.integrated.shellIntegration.enabled": false,
	"vim.handleKeys": {
		"<C-p>": false,
		"<C-f>": false,
		"<C-k>": false,
		"<C-b>": false
	},
	"vim.useSystemClipboard": true,
	"window.commandCenter": false,
	"window.enableMenuBarMnemonics": false,
	"window.menuBarVisibility": "hidden",
	"window.title": "${dirty}${activeEditorShort}${separator}${rootName}",
	"workbench.activityBar.location": "top",
	"workbench.colorTheme": "Catppuccin Mocha",
	"workbench.editor.pinnedTabSizing": "shrink",
	"workbench.layoutControl.enabled": false,
	"workbench.panel.defaultLocation": "right",
	"workbench.panel.opensMaximized": "always",
	"workbench.startupEditor": "none",
	"workbench.tree.renderIndentGuides": "none",
	"zenMode.centerLayout": false,
	"zenMode.hideStatusBar": false,
	"zenMode.showTabs": "single",
	"window.customMenuBarAltFocus": false,
	"window.titleBarStyle": "custom",
	"editor.fontSize": 16,
	"workbench.editor.showTabs": "none",
	"breadcrumbs.enabled": false,
	"editor.stickyScroll.enabled": false,
	"terminal.integrated.fontSize": 16,
	"extensions.experimental.affinity": {
		"asvetliakov.vscode-neovim": 1
	},
	"vim.gdefault": true,
	"vim.showmodename": false,
	"vscode-neovim.ctrlKeysForNormalMode": [
		"a",
		"c",
		"d",
		"e",
		"h",
		"i",
		"j",
		"k",
		"l",
		"m",
		"o",
		"r",
		"t",
		"u",
		"v",
		"w",
		"x",
		"y",
		"z",
		"/",
		"]",
		"right",
		"left",
		"up",
		"down",
		"backspace",
		"delete"
	],
	"vscode-neovim.neovimClean": true,
}
