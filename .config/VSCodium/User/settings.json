{
    "workbench.editor.showTabs": "single",
    "editor.minimap.renderCharacters": false,
    "editor.minimap.enabled": false,
    "editor.fontFamily": "'Fira Code', 'FiraCode Nerd Font', 'monospace', monospace",
    "gitlens.currentLine.enabled": false,
    "gitlens.currentLine.pullRequests.enabled": false,
    "editor.inlayHints.enabled": "off",
    "editor.scrollbar.horizontal": "hidden",
    "editor.scrollbar.vertical": "hidden",
    "editor.lightbulb.enabled": false,
    "terminal.integrated.fontFamily": "'Fira Code', 'FiraCode Nerd Font', 'monospace', monospace",
    "terminal.integrated.defaultLocation": "editor",
    "gitlens.graph.layout": "editor",
    "terminal.integrated.enableFileLinks": "off",
    "terminal.integrated.hideOnStartup": "always",
    "terminal.integrated.enablePersistentSessions": false,
    "terminal.external.linuxExec": "alacritty",
    "gitlens.codeLens.enabled": false,
    "gitlens.hovers.enabled": false,
    "gitlens.showWelcomeOnInstall": false,
    "gitlens.showWhatsNewAfterUpgrades": false,
    "gitlens.terminalLinks.enabled": false,
    "gitlens.telemetry.enabled": false,
    "gitlens.mode.statusBar.enabled": false,
    "git.openRepositoryInParentFolders": "never",
    "editor.tabSize": 2,
    "remote.autoForwardPorts": false,
    "explorer.excludeGitIgnore": true,
    "workbench.iconTheme": "vscode-icons",
    "workbench.colorTheme": "One Dark Pro Darker",
    "vsicons.dontShowNewVersionMessage": true,
    "editor.fontSize": 16,
    "vim.handleKeys": {
        "<C-p>": false,
        "<C-b>": false,
        "<C-w>": false,
        "<C-s>": false,
        "<C-z>": false,
    },
    "task.allowAutomaticTasks": "off",
    "task.autoDetect": "off",
    "task.reconnection": false,
    "task.saveBeforeRun": "never",
    "problems.autoReveal": false,
    "problems.decorations.enabled": false,
    "explorer.autoReveal": false,
    "editor.fontLigatures": true,
    "editor.parameterHints.enabled": false,,
    "rust-analyzer.semanticHighlighting.operator.enable": false,
    "rust-analyzer.semanticHighlighting.strings.enable": false,
    "rust-analyzer.semanticHighlighting.nonStandardTokens": false,
    "rust-analyzer.hover.actions.enable": false,
    "rust-analyzer.lens.enable": false,
    "window.customMenuBarAltFocus": false,
    "window.enableMenuBarMnemonics": false,
    "window.menuBarVisibility": "hidden",
    "editor.occurrencesHighlight": false,
    "editor.guides.highlightActiveBracketPair": false,
    "editor.foldingHighlight": false,
    "editor.selectionHighlight": false,
    "editor.matchBrackets": "never"
}