{
	"breadcrumbs.enabled": false,
	"editor.codeLens": false,
	"editor.dragAndDrop": false,
	"editor.folding": false,
	"editor.fontFamily": "'Fira Code', monospace",
	"editor.fontLigatures": "'zero','cv14','cv26','cv30','cv31','cv32','ss03','ss04','ss06','ss10'",
	"editor.guides.indentation": false,
	"editor.hover.enabled": false,
	"editor.inlayHints.enabled": "off",
	"editor.lightbulb.enabled": "off",
	"editor.matchBrackets": "never",
	"editor.minimap.enabled": false,
	"editor.scrollbar.horizontal": "hidden",
	"editor.scrollbar.vertical": "hidden",
	"editor.selectionHighlight": false,
	"editor.tabSize": 2,
	"editor.wordWrap": "on",
	"explorer.confirmDelete": false,
	"explorer.excludeGitIgnore": true,
	"git.confirmSync": false,
	"git.openRepositoryInParentFolders": "never",
	"remote.autoForwardPorts": false,
	"rust-analyzer.lens.enable": false,
	"rust-analyzer.semanticHighlighting.nonStandardTokens": false,
	"rust-analyzer.semanticHighlighting.operator.enable": false,
	"rust-analyzer.semanticHighlighting.strings.enable": false,
	"screencastMode.fontSize": 42,
	"screencastMode.keyboardOptions": {
		"showCommands": false
	},
	"screencastMode.keyboardOverlayTimeout": 2500,
	"screencastMode.mouseIndicatorSize": 40,
	"screencastMode.verticalOffset": 10,
	"svelte.ask-to-enable-ts-plugin": false,
	"terminal.integrated.enableFileLinks": "off",
	"terminal.integrated.enablePersistentSessions": false,
	"terminal.integrated.fontFamily": "'Fira Code', 'FiraCode Nerd Font', monospace",
	"terminal.integrated.hideOnStartup": "always",
	"terminal.integrated.shellIntegration.enabled": false,
	"vim.handleKeys": {
		"<C-p>": false,
		"<C-f>": false,
		"<C-k>": false,
		"<C-b>": false
	},
	"vim.useSystemClipboard": true,
	"window.commandCenter": false,
	"window.enableMenuBarMnemonics": false,
	"window.menuBarVisibility": "hidden",
	"window.title": "${dirty}${activeEditorShort}${separator}${rootName}",
	"workbench.activityBar.location": "top",
	"workbench.colorTheme": "Catppuccin Mocha",
	"workbench.editor.pinnedTabSizing": "shrink",
	"workbench.layoutControl.enabled": false,
	"workbench.panel.defaultLocation": "right",
	"workbench.panel.opensMaximized": "always",
	"workbench.startupEditor": "none",
	"workbench.tree.renderIndentGuides": "none",
	"zenMode.centerLayout": false,
	"zenMode.hideStatusBar": false,
	"zenMode.showTabs": "none",
	"window.customMenuBarAltFocus": false,
	"window.titleBarStyle": "custom",
	"explorer.confirmDragAndDrop": false,
	"git.ignoreMissingGitWarning": true,
	"editor.occurrencesHighlight": "off",
	"git.replaceTagsWhenPull": true,
	"workbench.editor.showTabs": "none",
	"workbench.tree.enableStickyScroll": false,
	"editor.stickyScroll.enabled": false,
	"terminal.integrated.rescaleOverlappingGlyphs": true,
	"editor.cursorBlinking": "solid",
	"rust-analyzer.showRequestFailedErrorNotification": false,
	"rust-analyzer.showUnlinkedFileNotification": false,
	"rust-analyzer.notifications.cargoTomlNotFound": false,
	"extensions.ignoreRecommendations": true,
	"diffEditor.ignoreTrimWhitespace": true,
	"editor.renderWhitespace": "none",
	"files.trimTrailingWhitespace": true,
	"editor.hideCursorInOverviewRuler": true,
	"editor.overviewRulerBorder": false,
}
